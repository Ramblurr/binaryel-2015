<html>
    <head>
        <title><%= current_page.data.title %></title>
        <meta http-equiv="content-type" content="text/html; charset=utf-8" />
        <meta name="description" content="" />
        <meta name="keywords" content="" />
        <!--[if lte IE 8]><script src="javascripts/directive/ie/html5shiv.js"></script><![endif]-->
        <script src="javascripts/directive/jquery.min.js"></script>
        <script src="javascripts/directive/skel.min.js"></script>
        <script src="javascripts/directive/init.js"></script>
        <noscript>
            <link rel="stylesheet" href="stylesheets/directive/skel.css" />
            <link rel="stylesheet" href="stylesheets/directive/style.css" />
            <link rel="stylesheet" href="stylesheets/directive/style-wide.css" />
            <link rel="stylesheet" href="stylesheets/all.css" />
        </noscript>
        <!--[if lte IE 8]><link rel="stylesheet" href="stylesheets/directive/ie/v8.css" /><![endif]-->
    </head>
    <body>

        <!-- Header -->
            <div id="header">
                <%#<h1>Hi. This is Directive.</h1>%>
                <%#<p>A responsive HTML5 + CSS3 site template designed by <a href="http://html5up.net">HTML5 UP</a>%>
                <%#<br />%>
                <%#and released for free under the <a href="http://html5up.net/license">Creative Commons license</a>.</p>%>
                <div class="title"></div>
                <div id="cloudcity">
                    <img  class="city" src="images/cloud-city-450.png"/>
                    <img  class="banner" src="images/banner1.png"/>
                    <img  class="banner2" src="images/banner2.png"/>
                </div>
                <div class="cloud-one"></div>
                <div class="cloud-two"></div>
                <div class="cloud-three"></div>
                <div class="cloud-four"></div>
            </div>

        <!-- Main -->
<div id="main">
 <%= yield %>
</div>

        <!-- Footer -->
            <div id="footer">
                <div class="container 75%">

                    <header class="major last">
                        <h2>Get in touch</h2>
                    </header>

                    <p></p>

                    <form method="post" action="#">
                        <div class="row">
                            <div class="6u 12u(mobilep)">
                                <input type="text" name="name" placeholder="Name" />
                            </div>
                            <div class="6u 12u(mobilep)">
                                <input type="email" name="email" placeholder="Email" />
                            </div>
                        </div>
                        <div class="row">
                            <div class="12u">
                                <textarea name="message" placeholder="Message" rows="6"></textarea>
                            </div>
                        </div>
                        <div class="row">
                            <div class="12u">
                                <ul class="actions">
                                    <li><input type="submit" value="Send Message" /></li>
                                </ul>
                            </div>
                        </div>
                    </form>

                    <ul class="icons">
                        <li><a href="https://twitter.com/Ramblurr" class="icon fa-twitter"><span class="label">Twitter</span></a></li>
                        <li><a href="https://github.com/Ramblurr" class="icon fa-github"><span class="label">Github</span></a></li>
                    </ul>

                    <ul class="copyright">
                        <li>made with <i class="icon fa-heart"></i> by Casey Link</li>
                        <li>&copy; 2015 <a href="http://outskirtslabs.com" title="Outskirts Labs | Technology for Good">Outskirts Labs</a></li>
                        <li>Credits: <a href="http://html5up.net">HTML5 UP</a></li>
                    </ul>

                </div>
            </div>
        <script src="javascripts/all.js"></script>
    </body>
</html>
